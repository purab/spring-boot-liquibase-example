<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="Purab the developer">
        <addColumn schemaName="liquibase"
                   tableName="users">
            <column name="address01" type="varchar(255)" />
        </addColumn>
    </changeSet>

    <changeSet author="Purab the developer" id="6">
        <delete schemaName="liquibase"
                tableName="users">
            <where>name='purab'</where>
        </delete>
    </changeSet>
    <changeSet author="Purab the developer" id="8">
        <createTable
                schemaName="liquibase"
                tableName="car"
                tablespace="A String">
            <column name="number-plate" type="varchar(255)"/>
        </createTable>
    </changeSet>


</databaseChangeLog>